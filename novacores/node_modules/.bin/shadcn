#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kaua/Documentos/Programação/img-ly/novacores/node_modules/.pnpm/shadcn@3.7.0_@types+node@20.19.30_hono@4.11.7_typescript@5.9.3/node_modules/shadcn/dist/node_modules:/home/kaua/Documentos/Programação/img-ly/novacores/node_modules/.pnpm/shadcn@3.7.0_@types+node@20.19.30_hono@4.11.7_typescript@5.9.3/node_modules/shadcn/node_modules:/home/kaua/Documentos/Programação/img-ly/novacores/node_modules/.pnpm/shadcn@3.7.0_@types+node@20.19.30_hono@4.11.7_typescript@5.9.3/node_modules:/home/kaua/Documentos/Programação/img-ly/novacores/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kaua/Documentos/Programação/img-ly/novacores/node_modules/.pnpm/shadcn@3.7.0_@types+node@20.19.30_hono@4.11.7_typescript@5.9.3/node_modules/shadcn/dist/node_modules:/home/kaua/Documentos/Programação/img-ly/novacores/node_modules/.pnpm/shadcn@3.7.0_@types+node@20.19.30_hono@4.11.7_typescript@5.9.3/node_modules/shadcn/node_modules:/home/kaua/Documentos/Programação/img-ly/novacores/node_modules/.pnpm/shadcn@3.7.0_@types+node@20.19.30_hono@4.11.7_typescript@5.9.3/node_modules:/home/kaua/Documentos/Programação/img-ly/novacores/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/shadcn@3.7.0_@types+node@20.19.30_hono@4.11.7_typescript@5.9.3/node_modules/shadcn/dist/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/shadcn@3.7.0_@types+node@20.19.30_hono@4.11.7_typescript@5.9.3/node_modules/shadcn/dist/index.js" "$@"
fi
