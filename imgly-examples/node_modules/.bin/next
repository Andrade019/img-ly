#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/next@15.1.9_@babel+core@7.28.6_react-dom@19.0.1_react@19.0.1__react@19.0.1/node_modules/next/dist/bin/node_modules:/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/next@15.1.9_@babel+core@7.28.6_react-dom@19.0.1_react@19.0.1__react@19.0.1/node_modules/next/dist/node_modules:/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/next@15.1.9_@babel+core@7.28.6_react-dom@19.0.1_react@19.0.1__react@19.0.1/node_modules/next/node_modules:/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/next@15.1.9_@babel+core@7.28.6_react-dom@19.0.1_react@19.0.1__react@19.0.1/node_modules:/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/next@15.1.9_@babel+core@7.28.6_react-dom@19.0.1_react@19.0.1__react@19.0.1/node_modules/next/dist/bin/node_modules:/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/next@15.1.9_@babel+core@7.28.6_react-dom@19.0.1_react@19.0.1__react@19.0.1/node_modules/next/dist/node_modules:/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/next@15.1.9_@babel+core@7.28.6_react-dom@19.0.1_react@19.0.1__react@19.0.1/node_modules/next/node_modules:/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/next@15.1.9_@babel+core@7.28.6_react-dom@19.0.1_react@19.0.1__react@19.0.1/node_modules:/home/kaua/Documentos/imgly-examples/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
